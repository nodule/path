{
  "name": "path",
  "description": "Path components for ChiÏ‡",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/path.git"
  },
  "nodes": [
    {
      "ports": {
        "input": {
          "path": {
            "title": "Path",
            "type": "string"
          },
          "ext": {
            "title": "Extension",
            "type": "string"
          }
        },
        "output": {
          "result": {
            "title": "Result",
            "type": "string"
          }
        }
      },
      "phrases": {
        "active": "Getting basename for {{input.path}} and extension {{input.ext}}"
      },
      "require": {
        "path": "latest"
      },
      "description": "Path basename",
      "ns": "path",
      "name": "basename",
      "fn": "output = { result: path.basename(input.path, input.ext) }\n"
    },
    {
      "ports": {
        "input": {
          "path": {
            "title": "Path",
            "type": "string"
          }
        },
        "output": {
          "result": {
            "title": "Result",
            "type": "string"
          }
        }
      },
      "require": {
        "path": "latest"
      },
      "phrases": {
        "active": "Getting dirname for {{input.path}}"
      },
      "description": "Path dirname",
      "ns": "path",
      "name": "dirname",
      "fn": "output = { result: path.dirname(input.path) }\n"
    },
    {
      "ports": {
        "input": {
          "path": {
            "title": "Path",
            "type": "string"
          }
        },
        "output": {
          "result": {
            "title": "Result",
            "type": "string"
          }
        }
      },
      "require": {
        "path": "latest"
      },
      "phrases": {
        "active": "Determine extension for {{input.path}}"
      },
      "description": "Path extname",
      "ns": "path",
      "name": "extname",
      "fn": "output = { result: path.extname(input.path) }\n"
    },
    {
      "ports": {
        "input": {
          "in": {
            "title": "Path",
            "type": "array"
          }
        },
        "output": {
          "result": {
            "title": "Result",
            "type": "string"
          }
        }
      },
      "require": {
        "path": "latest"
      },
      "phrases": {
        "active": "Joining path for {{input.path}}"
      },
      "description": "Path join",
      "ns": "path",
      "name": "join",
      "fn": "output = { result: path.join.apply(null, input.in) }\n"
    },
    {
      "ports": {
        "input": {
          "path": {
            "title": "Path",
            "type": "string"
          }
        },
        "output": {
          "result": {
            "title": "Result",
            "type": "string"
          }
        }
      },
      "require": {
        "path": "latest"
      },
      "phrases": {
        "active": "Normalizing path for {{input.path}}"
      },
      "description": "Path normalize",
      "ns": "path",
      "name": "normalize",
      "fn": "output = { result: path.normalize(input.path) }\n"
    },
    {
      "ports": {
        "input": {
          "from": {
            "title": "From",
            "type": "string"
          },
          "to": {
            "title": "To",
            "type": "string"
          }
        },
        "output": {
          "result": {
            "title": "Result",
            "type": "string"
          }
        }
      },
      "require": {
        "path": "latest"
      },
      "phrases": {
        "active": "Determining relative path for {{input.path}}"
      },
      "description": "Path relative",
      "ns": "path",
      "name": "relative",
      "fn": "output = { result: path.relative(input.from, input.to) }\n"
    },
    {
      "ports": {
        "input": {
          "from": {
            "title": "From",
            "type": "string"
          },
          "to": {
            "title": "To",
            "type": "string"
          }
        },
        "output": {
          "result": {
            "title": "Result",
            "type": "string"
          }
        }
      },
      "require": {
        "path": "latest"
      },
      "phrases": {
        "active": "Resolving path for {{input.path}}"
      },
      "description": "Path resolve",
      "ns": "path",
      "name": "resolve",
      "fn": "output = { result: path.resolve(input.from, input.to) }\n"
    }
  ],
  "twigs": []
}