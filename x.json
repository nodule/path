{
  "name": "path",
  "description": "Path components for ChiÏ‡",
  "version": "0.0.1",
  "licences": [
    {
      "type": "MIT",
      "url": "https://github.com/psichi/chix/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/psichi/chix-path.git"
  },
  "nodes": [
    {
      "ports": {
        "input": {
          "path": {
            "type": "string"
          },
          "ext": {
            "type": "string"
          }
        },
        "output": {
          "result": {
            "type": "string"
          }
        }
      },
      "description": "Path basename",
      "name": "basename",
      "ns": "path",
      "fn": "output = { result: path.basename(input.path, input.ext) }\n"
    },
    {
      "ports": {
        "input": {
          "path": {
            "type": "string"
          }
        },
        "output": {
          "result": {
            "type": "string"
          }
        }
      },
      "description": "Path dirname",
      "name": "dirname",
      "ns": "path",
      "fn": "output = { result: path.dirname(input.path) }\n"
    },
    {
      "ports": {
        "input": {
          "path": {
            "type": "string"
          }
        },
        "output": {
          "result": {
            "type": "string"
          }
        }
      },
      "description": "Path extname",
      "name": "extname",
      "ns": "path",
      "fn": "output = { result: path.extname(input.path) }\n"
    },
    {
      "ports": {
        "input": {
          "path": {
            "type": "string"
          }
        },
        "output": {
          "result": {
            "type": "string"
          }
        }
      },
      "description": "Path join",
      "name": "join",
      "ns": "path",
      "fn": "output = { result: path.join(input.path) }\n"
    },
    {
      "ports": {
        "input": {
          "path": {
            "type": "string"
          }
        },
        "output": {
          "result": {
            "type": "string"
          }
        }
      },
      "description": "Path normalize",
      "name": "normalize",
      "ns": "path",
      "fn": "output = { result: path.normalize(input.path) }\n"
    },
    {
      "ports": {
        "input": {
          "from": {
            "type": "string"
          },
          "to": {
            "type": "string"
          }
        },
        "output": {
          "result": {
            "type": "string"
          }
        }
      },
      "description": "Path relative",
      "name": "relative",
      "ns": "path",
      "fn": "output = { result: path.relative(input.from, input.to) }\n"
    },
    {
      "ports": {
        "input": {
          "from": {
            "type": "string"
          },
          "to": {
            "type": "string"
          }
        },
        "output": {
          "result": {
            "type": "string"
          }
        }
      },
      "description": "Path resolve",
      "name": "resolve",
      "ns": "path",
      "fn": "output = { result: path.resolve(input.from, input.to) }\n"
    }
  ],
  "twigs": []
}