{
  "path.basename": {
    "fn": "output = { result: path.basename(input.path, input.ext) }\n",
    "ports": {
      "input": {
        "path": {
          "type": "string"
        },
        "ext": {
          "type": "string"
        }
      },
      "output": {
        "result": {
          "type": "string"
        }
      }
    },
    "description": "Path basename",
    "name": "path.basename"
  },
